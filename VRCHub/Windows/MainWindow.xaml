                        <Button.BorderBrush>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF3A0000"/>
                                <GradientStop Color="#FFFF0B0B" Offset="1"/>
                            </LinearGradientBrush>
                        </Button.BorderBrush>
