                        <Button.BorderBrush>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF770000"/>
                                <GradientStop Color="Red" Offset="1"/>
                            </LinearGradientBrush>
